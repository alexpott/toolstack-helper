{
    "name": "mglaman/toolstack-helper",
    "description": "Helps identify what kind of project is in a folder (Drupal, WordPress, Symfonfy, etc.)",
    "require": {
        "symfony/process": "2.7.*",
        "symfony/finder": "2.7.*",
        "symfony/filesystem": "2.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mikey179/vfsStream": "1.4"
    },
    "autoload": {
      "psr-4": {
        "mglaman\\Toolstack\\": "src"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "mglaman\\Toolstack\\Tests\\": "src"
        }
    },
